{
  "$schema": "https://raw.githubusercontent.com/microsoft/react-native-test-app/trunk/schema.json",
  "name": "example",
  "displayName": "example",
  "components": [
    {
      "appKey": "example",
      "displayName": "example"
    }
  ],
  "android": {
    "package": "com.example",
    "signingConfigs": {
      "debug": {
        "storeFile": "android/debug.keystore"
      },
      "release": {
        "storeFile": "android/debug.keystore"
      }
    }
  },
  "ios": {
    "bundleIdentifier": "com.example"
  },
  "macos": {
    "bundleIdentifier": "com.example"
  },
  "resources": {
    "android": [
      "dist/res",
      "dist/main.android.jsbundle"
    ],
    "ios": [
      "dist/assets",
      "dist/main.ios.jsbundle"
    ],
    "macos": [
      "dist/assets",
      "dist/main.macos.jsbundle"
    ],
    "windows": [
      "dist/assets",
      "dist/main.windows.bundle"
    ]
  }
}
