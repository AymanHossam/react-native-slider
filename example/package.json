{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "windows": "react-native run-windows",
    "refresh-package": "rimraf ./node_modules/local-slider-package && npx copyfiles -u 2 \"./../package/**/*\" ./node_modules/local-slider-package && rimraf ./node_modules/local-slider-package/node_modules",
    "clean-android": "rm -rf android/app/build",
    "clean-ios": "rm -rf ios/build/generated/ios && rm -rf ios/Pods && rm ios/Podfile.lock"
  },
  "dependencies": {
    "@react-native-community/slider": "^4.5.0",
    "babel-preset-expo": "^9.2.0",
    "local-slider-package": "file:../package",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-windows": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.69.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^29.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "patch-package": "^8.0.0",
    "prettier": "^2.4.1",
    "react-native-pager-view": "^6.1.1",
    "react-test-renderer": "18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
